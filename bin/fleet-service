#!/usr/bin/env node

var program =  require('commander');

program
  .version("0.0.1")
  .command('dec [options]', 'Decrement the number of replicas for a service')
  .command('inc [options]', 'Increment the number of replicas for a service')
  .command('list', 'List the services in the Data Center')
  .parse(process.argv);

