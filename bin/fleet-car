#!/usr/bin/env node

var program =  require('commander');

program
  .version("0.0.1")
  .command('add [options]', 'Create a Car in the simulation.')
  .command('list', 'List all of the applications')
  .command('remove [options]', 'Remove a car from the simulation')
  .command('report [options]', 'Report Car telemetry')
  .command('update [options]', 'Update Car in the simulation')
  .parse(process.argv);

